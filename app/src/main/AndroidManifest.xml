<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguideapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainSitesActivity"
            android:label="Main Sites"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ArchaeoActivity"
            android:label="Archaeological"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".HistoricalActivity"
            android:label="Historical"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FactsActivity"
            android:label="Jordan Facts"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>